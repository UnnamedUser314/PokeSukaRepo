<UserControl x:Class="PokemonBackRules.View.ImportView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PokemonBackRules.View" 
             xmlns:viewmodel="clr-namespace:PokemonBackRules.ViewModel"
             d:DataContext="{d:DesignInstance Type=viewmodel:ImportViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Background="Wheat">
    <Grid>
        <Button Content="Import Json Sukitones" Command="{Binding LoadFromFileCommand}" HorizontalAlignment="Center" Margin="0,63,0,0" VerticalAlignment="Top" Height="62" Width="160" RenderTransformOrigin="0.5,0.5">

        </Button>
        <Image HorizontalAlignment="Left" Height="100" Margin="348,189,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</UserControl>
